        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a52">
                <ANNOTATION_VALUE> we played cards //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
